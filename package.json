{
  "name": "fitfeel",
  "version": "1.0.0",
  "description": "App to check-in mentally before and after workouts.",
  "engines": {
    "node": "10.15.1",
    "npm": "6.9.0"
  },
  "main": "server.js",
  "scripts": {
    "test": "export NODE_ENV=test && jasmine",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "client": "cd ./client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piontekle/fitfeel.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/piontekle/fitfeel/issues"
  },
  "homepage": "https://github.com/piontekle/fitfeel",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-remember-me": "0.0.1",
    "path": "^0.12.7",
    "pg": "^7.9.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.7.1",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "jasmine": "^3.4.0",
    "request": "^2.88.0"
  }
}
